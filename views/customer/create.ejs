<%- include("../partials/header.ejs") -%>
<form action="/student/create" method="POST">
  <div class="padding row form-group">
    <div class="col-md-4">
      <label for="userName"><h3>Name</h3></label>
      <input type="text" class="form-control" id="userName" name="name" />
    </div>
  </div>
  <div class="padding row form-group">
    <div class="col-md-4">
      <label for="num"><h3>ID</h3></label>
      <input type="number" class="form-control" id="num" name="no" />
    </div>
  </div>
  <button type="submit" class="padding btn btn-lg btn-primary">Go!</button>
</form>
<%- include("../partials/footer.ejs") -%>
