<%- include("../partials/header") -%>
<h1>The Customer Table:</h1>
<table class="styled-table">
  <thead>
    <tr>
      <th>Customer ID</th>
      <th>Model ID</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(row => { %>
    <tr>
      <td><%= row.CustomerID %></td>
      <td><%= row.ModelID %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
<h1>Do you want to:</h1>
<div class="row padding list-group">
  <a href="../customerModel/create" class="col-sm-3 padding-sm list-group-item list-group-item-info">Create a record</a>
  <a href="../customerModel/update"
    class="col-sm-3 padding-sm list-group-item list-group-item-action list-group-item-info">Update a record</a>
  <a href="../customerModel/delete"
    class="col-sm-3 padding-sm list-group-item list-group-item-action list-group-item-info">Delete a record</a>
</div>

<%- include("../partials/footer") -%>